using System;

namespace _200B
{
    class Program
    {
        static void Main(string[] args)
        {
            int t = Convert.ToInt32(Console.ReadLine());
            string[] str = (Console.ReadLine()).Split(' ');
            double sum = 0;
            for(int i=0;i<t;i++)
            {

                sum += int.Parse(str[i]);
            }
            Console.WriteLine((sum/t).ToString());
        }
    }
}
